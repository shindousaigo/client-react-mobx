    "axios": "^0.18.0",
    "gsap": "^2.0.2",
    "iscroll": "^5.2.0",
    "ismobilejs": "^0.4.1",
    "jsonp": "^0.2.1",
    "lodash": "^4.17.10",
    "md5": "^2.2.1",
    "mobx": "4.3.1",
    "mobx-react": "5.2.3",
    "query-string": "^6.1.0",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-throttle": "^0.3.0",
    "routes": "2.1.0",
    "rsuite": "^3.2.9"
  },
