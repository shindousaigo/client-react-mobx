    "@types/jsonp": "^0.2.0",
    "@types/lodash": "^4.14.116",
    "@types/node": "10.5.1",
    "@types/query-string": "^6.1.0",
    "@types/react": "16.4.6",
    "@types/react-dom": "16.0.6",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "@types/underscore": "^1.8.9",
    "autoprefixer": "8.6.4",
